   "outputs": [],
   "source": [
    "for i in range(1, len(a)):\n",
    "    current = a[i]\n",
    "    j = i - 1\n",
    "    while j >= 0 and a[j] > current:\n",
    "        a[j + 1] = a[j]\n",
    "        j -= 1\n",
    "    a[j + 1] = current"
   ]
